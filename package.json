{
  "name": "ng112-js",
  "version": "1.5.1",
  "license": "AGPL-3.0-only",
  "author": "Gabriel Unterholzer <gabriel.unterholzer@dec112.at> (https://www.dec112.at)",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:dec112/ng112-js.git"
  },
  "scripts": {
    "start": "ENV_TYPE=browser tsdx watch --format esm --name index",
    "build": "node ./scripts/build.js",
    "build:browser": "ENV_TYPE=browser tsdx build --target web --format esm --name index",
    "build:node": "ENV_TYPE=node tsdx build --target node --format cjs --name index",
    "docs": "typedoc",
    "docs-deploy": "node scripts/docs.deploy.js",
    "test": "sh ./scripts/test.sh",
    "test:tsdx": "tsdx test",
    "test:clear-cache": "./node_modules/jest/bin/jest.js --clearCache",
    "prepublishOnly": "npm run docs-deploy && npm run build"
  },
  "devDependencies": {
    "find-root": "^1.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typedoc": "^0.22.10",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.7.5",
    "jssip": "^3.8.2",
    "pidf-lo": "^0.9.0"
  },
  "peerDependencies": {
    "jssip-node-websocket": "^3.0.0"
  }
}
