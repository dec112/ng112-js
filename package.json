  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:dec112/ng112-js.git"
  },
  "scripts": {
    "start": "ENV_TYPE=browser tsdx watch --format esm --name index",
    "build": "node ./scripts/build.js",
    "build:browser": "ENV_TYPE=browser tsdx build --target web --format esm --name index",
    "build:node": "ENV_TYPE=node tsdx build --target node --format cjs --name index",
    "docs": "typedoc",
    "docs-deploy": "node scripts/docs.deploy.js",
    "test": "sh ./scripts/test.sh",
    "test:tsdx": "tsdx test",
    "test:clear-cache": "./node_modules/jest/bin/jest.js --clearCache",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@xmldom/xmldom": "^0.7.5",
    "find-root": "^1.1.0",
    "jssip": "^3.7.5",
    "ng112-js-sip-adapter-jssip": "^0.5.0",
    "ng112-js-sip-adapter-sipjs": "^0.5.0",
    "rollup-plugin-modify": "^3.0.0",
    "sip.js": "^0.20.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typedoc": "^0.22.10",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@xstate/fsm": "^1.6.2",
    "pidf-lo": "^0.9.1"
  },
  "keywords": [
    "emergency",
    "911",
    "112",
    "ng911",
    "ng112",
    "dec112"
  ]
